---
export const prerender = false;

import Layout from "../layouts/layout.astro";
import NavBar from "../components/navbar.astro";
import OrderSection from "../components/orderSection.astro";
import TryOtherTreats from "../components/tryOtherTreats.astro";
const {item_name="Item Name", price="$0", url="src/assets/products-cake.png"} = Astro.props;

---

<Layout>
  <NavBar />

  <OrderSection 
    item_name={item_name}
    price={price}
    url={url}
  />
  <TryOtherTreats/>
</Layout>
