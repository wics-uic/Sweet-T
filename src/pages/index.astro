---
import NavBar from "../components/navbar.astro"
import Product from "../components/product.astro"
import Layout from "../layouts/layout.astro"

import Testimonial from "../components/testimonial.astro";

import '../styles/global.css';

import MessageCard from "../components/MessageCard.astro";
---

<html lang="en">
	<head>
		<meta charset="utf-8" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="viewport" content="width=device-width" />
		<meta name="generator" content={Astro.generator} />
		<title>Astro</title>
	</head>
	<Layout>
		
		<NavBar />
		<Testimonial name="Josh Perez"/>
		<Product url="src/assets/products-cupcake.png" item_name="cupcakes" price="$42/dozen"/>
	</Layout>
</html>

<html>
  <head>
    <title>My Page</title>
  </head>
  <body>
    <MessageCard message="Hello is this a component" />
  </body>
</html>

<a href="/myPage">
  <button>Go to My Page</button>
</a>